GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AFSoundManager" "${PODS_ROOT}/Headers/Public/MagicalRecord"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AFSoundManager" -isystem "${PODS_ROOT}/Headers/Public/MagicalRecord"
OTHER_LDFLAGS = $(inherited) -ObjC -l"AFSoundManager" -l"MagicalRecord" -framework "AVFoundation" -framework "AudioToolbox" -framework "CoreData" -framework "MediaPlayer"
PODS_ROOT = ${SRCROOT}/Pods